{
  "name": "Graphql-typescript-postgresql",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently \" nodemon ./dist/bundle.js \" \" webpack --watch --config webpack.config.js\" ",
    "build": "webpack",
    "test:integration": "jest /src -c  jest.config.js --forceExit ",
    "start": "node /dist/bundle.js",
    "migrate:up": "knex --knexfile ./src/database/knexfile.ts migrate:latest ",
    "test:migrate:up": "knex --knexfile ./src/database/knexfile.ts migrate:latest --env testing",
    "test:migrate:down": "knex --knexfile ./src/database/knexfile.ts migrate:down --env testing",
    "generate:types": "graphql-codegen --config codegen.yml",
    "test:seed-db": "knex --knexfile ./src/database/knexfile.ts seed:run --env testing"
  },
  "dependencies": {

    "apollo-server-express": "^2.16.1",
    "apollo-server-testing": "^2.16.1",

    "body-parser": "^1.19.0",
    "dataloader": "^2.0.0",
    "express": "^4.17.1",
    "graphql": "^15.0.0",
    "graphql-tools": "^6.0.0",
    "knex": "^0.21.1",
    "objection": "^2.1.3",
    "pg": "^8.2.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.14.0",
    "@graphql-codegen/typescript": "1.14.0",
    "@graphql-codegen/typescript-operations": "^1.14.0",
    "@graphql-codegen/typescript-resolvers": "1.14.0",
    "@jagi/jest-transform-graphql": "^1.0.2",
    "@types/express": "^4.17.6",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^26.0.3",
    "@types/mock-knex": "^0.4.3",
    "@types/node": "^14.0.1",
    "awesome-typescript-loader": "^5.2.1",
    "concurrently": "^5.2.0",
    "graphql-tag": "^2.10.3",
    "jest": "^26.2.1",
    "knex-cleaner": "^1.3.1",
    "nodemon": "^2.0.4",
    "ts-jest": "^26.1.1",
    "ts-node": "^8.10.1",
    "typescript": "^3.9.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2"
  }
}
